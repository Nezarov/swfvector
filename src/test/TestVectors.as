package test {	import wumedia.vector.VectorShapes;		import flash.display.Sprite;	import flash.display.StageAlign;	import flash.display.StageScaleMode;	import flash.events.Event;	import flash.net.URLLoader;	import flash.net.URLLoaderDataFormat;	import flash.net.URLRequest;		/**	 * @author guojian@wu-media.com | guojian.wu@ogilvy.com	 */	public class TestVectors extends Sprite {				public function TestVectors():void {			if (stage) init();			else addEventListener(Event.ADDED_TO_STAGE, init);		}				private var _loader	:URLLoader;				private function init(e:Event = null):void {			removeEventListener(Event.ADDED_TO_STAGE, init);			stage.scaleMode = StageScaleMode.NO_SCALE;			stage.align = StageAlign.TOP_LEFT;									_loader = new URLLoader();			_loader.dataFormat = URLLoaderDataFormat.BINARY;			_loader.addEventListener(Event.COMPLETE, onLoaded);			_loader.load(new URLRequest("vectorassets.swf"));					}				private function onLoaded(e:Event):void {			VectorShapes.extractFromLibrary(_loader.data, ["circles"]);			VectorShapes.draw(graphics, "circles", 1.0, 200, 200);		}	}	}